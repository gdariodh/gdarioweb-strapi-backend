      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "default": "diazhernandezgabriel2001@gmail.com"
    },
    "picture": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "gallery": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
